{
  "$schema": "./tsconfig-schema.json",
  "extends": "astro/tsconfigs/strict",
  "include": ["**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "rootDirs": [".", "cmk"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*", "cmk/src/*"]
    }
  },
  "cmkOptions": {
    "dtsOutDir": "cmk"
  }
}
