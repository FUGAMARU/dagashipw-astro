---
/** Props */
type Props = {
  /** Ad Slot */
  adSlot: string
}

const { adSlot } = Astro.props
---

<ins
  class="adsbygoogle"
  data-ad-client="ca-pub-5828524546032275"
  data-ad-format="fluid"
  data-ad-layout="in-article"
  data-ad-slot={adSlot}
  data-full-width-responsive="false"
  style="display:block; text-align:center;"></ins>
